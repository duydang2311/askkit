{
  "identifier": "desktop-capability",
  "platforms": [
    "macOS",
    "windows",
    "linux"
  ],
  "windows": [
    "launcher"
  ],
  "permissions": [
    "global-shortcut:default",
    "core:window:allow-set-size",
    "core:window:allow-theme",
    "core:event:allow-listen",
    "core:event:allow-unlisten",
    "stronghold:default"
  ]
}